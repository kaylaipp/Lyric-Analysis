weekday <- function(day,month,year){
y <- year%%100
c <- substr(year,1, nchar(year)-2)
y <- as.numeric(y)
c <- as.numeric(c)
result <- ((2.6*month-0.2) + day + year + (y/4) + (c/4)-2*c )%%7
if(result == 1){
print('Sunday')
}
if(result == 2){
print('Monday')
}
if(result == 3){
print('Tuesday')
}
if(result == 4){
print('Wednesday')
}
if(result == 5){
print('Thursday')
}
if(result == 6){
print('Friday')
}
if(result == 7){
print('Saturday')
}
}
weekday(1,2,2018)
